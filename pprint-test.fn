
(
(eql "123" (pprint 123))
(eql "\"abc\"" (pprint "abc"))
(eql "\\a" (pprint #\a))
(eql "(1 2 3)" (pprint '(1 2 3)))
(eql "foo" (pprint 'foo))
(eql "nil" (pprint nil))

(eql "a b c" (string-join (list "a" "b" "c") " "))
(eql '() (string-join (list) " "))
(eql "foo, bar, baz" (string-join (list "foo" "bar" "baz") ", "))
(eql "foobarbaz" (string-concat "foo" "bar" "baz"))

)