
(

(eql '(let ((x 4)
            (y 8))
       (+ x y))
     (map-letdefinitions (lambda (x) (+ 3 x))
			 '(let ((x 1)
				(y 5))
			   (+ x y))))

(eql '(+ 9 y)
     (replace-var '(+ x y) 'x 9))

; Don't replace beyond a let that redefines x
(eql '(+ 42 (let ((x 34)) x))
     (replace-var '(+ x (let ((x 34)) x)) 'x 42))

)
